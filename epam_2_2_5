package com.company;

public class Main {

    public static void main(String[] args) {
        int n = 5;
        int n2 = n;
        int [][] arr = new int [n][n];
        for(int i = 0;i < n;i++,n2--){
            for (int j = 0;j < n2;j++) {
                arr[i][j] = i + 1;
            }
        }
        for(int i = 0;i < n;i++){
            for (int j = 0; j < n; j++)
                System.out.print(arr[i][j] + " ");
            System.out.print("\n");
        }
    }
}
